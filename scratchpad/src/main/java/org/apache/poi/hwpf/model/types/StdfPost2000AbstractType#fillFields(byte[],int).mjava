    protected void fillFields( byte[] data, int offset )
    {
        field_1_info1                  = LittleEndian.getShort(data, 0x0 + offset);
        field_2_rsid                   = LittleEndian.getUInt(data, 0x2 + offset);
        field_3_info3                  = LittleEndian.getShort(data, 0x6 + offset);
    }

