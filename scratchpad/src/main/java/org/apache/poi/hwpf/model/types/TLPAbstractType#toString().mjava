            "         .fBorders                 = " + isFBorders() + "\n" +
            "         .fShading                 = " + isFShading() + "\n" +
            "         .fFont                    = " + isFFont() + "\n" +
            "         .fColor                   = " + isFColor() + "\n" +
            "         .fBestFit                 = " + isFBestFit() + "\n" +
            "         .fHdrRows                 = " + isFHdrRows() + "\n" +
            "         .fLastRow                 = " + isFLastRow() + "\n" +
