    private static void addStepTransition(List<ComparisonExample> temp, long rawBits) {
        Collections.addAll(temp, new ComparisonExample[]{
                ce(rawBits - 1, rawBits + 0, 0),
                ce(rawBits + 0, rawBits + 1, -1),
                ce(rawBits + 1, rawBits + 2, 0),
        });

    }

