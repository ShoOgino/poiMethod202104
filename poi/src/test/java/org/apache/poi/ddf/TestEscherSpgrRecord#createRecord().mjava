    private static EscherSpgrRecord createRecord()
    {
        EscherSpgrRecord r = new EscherSpgrRecord();
        r.setOptions( (short) 0x0010 );
        r.setRecordId( EscherSpgrRecord.RECORD_ID );
        r.setRectX1(1);
        r.setRectY1(2);
        r.setRectX2(3);
        r.setRectY2(4);
        return r;
    }

