    @Test
    void testPatterns() {
        // just try to trash the functionality with some byte-patterns
        for(int i = 0; i < 256;i++) {
            final byte[] data = new byte[12];
            for(int j = 0;j < 12; j++) {
                data[j] = (byte)i;

                assertEquals(FileMagic.UNKNOWN, FileMagic.valueOf(data));
            }
        }
    }

