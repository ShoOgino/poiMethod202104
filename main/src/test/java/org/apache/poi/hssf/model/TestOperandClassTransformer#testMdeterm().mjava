    @Test
    void testMdeterm() {
        String formula = "MDETERM(ABS(A1))";
        Ptg[] ptgs = parseFormula(formula);

        confirmTokenClass(ptgs, 0, Ptg.CLASS_ARRAY);
        confirmFuncClass(ptgs, 1, "ABS", Ptg.CLASS_ARRAY);
        confirmFuncClass(ptgs, 2, "MDETERM", Ptg.CLASS_VALUE);
    }

