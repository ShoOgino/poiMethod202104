    @Test
    void testIndexOf() {
        IntList list = new IntList();

        for (int j = 0; j < 1000; j++) {
            list.add(j / 2);
        }
        for (int j = 0; j < 1000; j++) {
            if (j < 500) {
                assertEquals(j * 2, list.indexOf(j));
            } else {
                assertEquals(-1, list.indexOf(j));
            }
        }
    }

