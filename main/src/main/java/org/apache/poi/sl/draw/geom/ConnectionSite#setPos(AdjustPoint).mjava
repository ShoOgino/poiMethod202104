    /**
     * Sets the value of the pos property.
     *
     * @param pos
     *     allowed object is
     *     {@link AdjustPoint }
     *
     */
    public void setPos(AdjustPoint pos) {
        if (pos != null) {
            this.pos.setX(pos.getX());
            this.pos.setY(pos.getY());
        }
    }

