    public byte getByteValue() {
        switch (this) {
            case DOUBLE:
                return Font.U_DOUBLE;
            case DOUBLE_ACCOUNTING:
                return Font.U_DOUBLE_ACCOUNTING;
            case SINGLE_ACCOUNTING:
                return Font.U_SINGLE_ACCOUNTING;
            case NONE:
                return Font.U_NONE;
            case SINGLE:
                return Font.U_SINGLE;
            default:
                return Font.U_SINGLE;
        }
    }

