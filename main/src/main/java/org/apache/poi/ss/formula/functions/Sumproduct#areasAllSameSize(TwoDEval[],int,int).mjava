	private static boolean areasAllSameSize(TwoDEval[] args, int height, int width) {
		for (TwoDEval areaEval : args) {
			// check that height and width match
			if (areaEval.getHeight() != height) {
				return false;
			}
			if (areaEval.getWidth() != width) {
				return false;
			}
		}
		return true;
	}

