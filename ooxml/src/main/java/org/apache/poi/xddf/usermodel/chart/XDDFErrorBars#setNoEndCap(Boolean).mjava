    public void setNoEndCap(Boolean noEndCap) {
        if (noEndCap == null) {
            if (bars.isSetNoEndCap()) {
                bars.unsetNoEndCap();
            }
        } else {
            if (bars.isSetNoEndCap()) {
                bars.getNoEndCap().setVal(noEndCap);
            } else {
                bars.addNewNoEndCap().setVal(noEndCap);
            }
        }
    }

