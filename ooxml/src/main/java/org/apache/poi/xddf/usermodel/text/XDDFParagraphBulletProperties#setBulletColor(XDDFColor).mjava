    public void setBulletColor(XDDFColor color) {
        if (props.isSetBuClrTx()) {
            props.unsetBuClrTx();
        }
        if (color == null) {
            if (props.isSetBuClr()) {
                props.unsetBuClr();
            }
        } else {
            props.setBuClr(color.getColorContainer());
        }
    }

