    /**
     * Add an {@link XWPFFootnote} to the document
     *
     * @param footnote Footnote to add
     * @throws IOException
     */
    public void addFootnote(XWPFFootnote footnote) {
        listFootnote.add(footnote);
        ctFootnotes.addNewFootnote().set(footnote.getCTFtnEdn());
    }

