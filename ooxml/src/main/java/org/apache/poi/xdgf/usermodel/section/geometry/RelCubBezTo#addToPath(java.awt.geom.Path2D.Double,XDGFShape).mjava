    @Override
    public void addToPath(java.awt.geom.Path2D.Double path, XDGFShape parent) {

        if (getDel())
            return;

        double w = parent.getWidth();
        double h = parent.getHeight();

        path.curveTo(getA() * w, getB() * h, getC() * w, getD() * h,
                getX() * w, getY() * h);
    }

