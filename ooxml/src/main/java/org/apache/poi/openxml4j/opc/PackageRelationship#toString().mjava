	@Override
	public String toString() {
		return "id=" + id +
				" - container=" + container +
				" - relationshipType=" + relationshipType +
				(source == null ? " - source=null" : " - source=" + getSourceURI().toASCIIString()) +
				" - target=" + getTargetURI().toASCIIString() +
				(targetMode == null ? ",targetMode=null" : ",targetMode=" + targetMode);
	}

