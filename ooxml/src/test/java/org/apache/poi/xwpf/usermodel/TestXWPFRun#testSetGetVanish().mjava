    @Test
    void testSetGetVanish() throws IOException {
        XWPFDocument document = new XWPFDocument();
        final XWPFRun run = document.createParagraph().createRun();
        assertFalse(run.isVanish());
        run.setVanish(true);
        assertTrue(run.isVanish());
        run.setVanish(false);
        assertFalse(run.isVanish());
        document.close();
    }

