    public void execute() {
        Project project = getProject();
        String relCurr = project.getProperty("version.id");
        Matcher m = pattern.matcher(relCurr);
        if (m.find()) {
            project.setProperty(property, m.group(1) + "." + m.group(2) + "." + (Integer.parseInt(m.group(3)) + increment));
        }
    }

