    public void setBulletFont(XDDFFont font) {
        if (props.isSetBuFontTx()) {
            props.unsetBuFontTx();
        }
        if (font == null) {
            if (props.isSetBuFont()) {
                props.unsetBuFont();
            }
        } else {
            props.setBuFont(font.getXmlObject());
        }
    }

