   public void setGapDepth(Integer depth) {
       if (depth == null) {
           if (chart.isSetGapDepth()) {
               chart.unsetGapDepth();
           }
       } else {
           if (chart.isSetGapDepth()) {
               chart.getGapDepth().setVal(depth);
           } else {
               chart.addNewGapDepth().setVal(depth);
           }
       }
   }

