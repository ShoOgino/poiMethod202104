    @Test
    public void testLastIndexOf() {
        IntList list = new IntList();

        for (int j = 0; j < 1000; j++) {
            list.add(j / 2);
        }
        for (int j = 0; j < 1000; j++) {
            if (j < 500) {
                assertEquals(1 + j * 2, list.lastIndexOf(j));
            } else {
                assertEquals(-1, list.indexOf(j));
            }
        }
    }

