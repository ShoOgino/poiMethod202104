	@Test
	void testValidNames() {
		assertTrue(isValid("Sheet1"));
		assertTrue(isValid("O'Brien's sales"));
		assertTrue(isValid(" data # "));
		assertTrue(isValid("data $1.00"));

		assertFalse(isValid("data?"));
		assertFalse(isValid("abc/def"));
		assertFalse(isValid("data[0]"));
		assertFalse(isValid("data*"));
		assertFalse(isValid("abc\\def"));
		assertFalse(isValid("'data"));
		assertFalse(isValid("data'"));
	}

