    @ParameterizedTest
    @MethodSource("files")
	void testMain(File file) throws Exception {
	    String fileName = file.getName();

	    Class<? extends Throwable> t = EXCLUDED.get(fileName);

        if (t == null) {
            runOneFile(file);
        } else {
            assertThrows(t, () -> runOneFile(file));
        }

	}

