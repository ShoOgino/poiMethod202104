    /** MissingArgPtg */
    @Test
    public void testMissingArgPtg() throws IOException {
        try (HSSFWorkbook wb = new HSSFWorkbook()) {
            HSSFCell cell = wb.createSheet("Sheet1").createRow(4).createCell(0);
            assertDoesNotThrow(() -> cell.setCellFormula("IF(A1=\"A\",1,)"));
        }
    }

