	private static void writeLong(byte[] bb, int i, long val) {
		String oldVal = interpretLong(bb, i);
		bb[i+7] = (byte) (val >> 56);
		bb[i+6] = (byte) (val >> 48);
		bb[i+5] = (byte) (val >> 40);
		bb[i+4] = (byte) (val >> 32);
		bb[i+3] = (byte) (val >> 24);
		bb[i+2] = (byte) (val >> 16);
		bb[i+1] = (byte) (val >>  8);
		bb[i+0] = (byte) (val >>  0);
		if (false) {
			String newVal = interpretLong(bb, i);
			System.out.println("changed offset " + i + " from " + oldVal + " to " + newVal);
		}
		
	}

