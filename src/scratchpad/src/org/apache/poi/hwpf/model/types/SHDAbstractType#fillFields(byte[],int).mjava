    protected void fillFields( byte[] data, int offset )
    {
        field_1_cvFore                 = new Colorref(data, 0x0 + offset);
        field_2_cvBack                 = new Colorref(data, 0x4 + offset);
        field_3_ipat                   = LittleEndian.getShort(data, 0x8 + offset);
    }

