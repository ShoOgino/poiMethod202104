    void emptyTextBox() {
    	assertEquals(2, pptB.getSlides().size());
    	HSLFSlide s1 = pptB.getSlides().get(0);
    	HSLFSlide s2 = pptB.getSlides().get(1);

    	// Check we can get the shapes count
    	assertEquals(2, s1.getShapes().size());
    	assertEquals(2, s2.getShapes().size());
    }

