    @Test
    void testLine() {

        Path2D.Double path1 = new Path2D.Double(new Line2D.Double(100, 100, 200, 100));

        HSLFFreeformShape p = new HSLFFreeformShape();
        p.setPath(path1);

        java.awt.Shape path2 = p.getPath();
        assertTrue(new Area(path1).equals(new Area(path2)));
    }

