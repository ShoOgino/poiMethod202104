   /**
    * If this Hyperlink is an external reference hyperlink,
    *  return the object for it.
    */
   public XWPFHyperlink getHyperlink(XWPFDocument document) {
      String id = getHyperlinkId();
      if(id == null)
         return null;
      
      return document.getHyperlinkByID(id);
   }

