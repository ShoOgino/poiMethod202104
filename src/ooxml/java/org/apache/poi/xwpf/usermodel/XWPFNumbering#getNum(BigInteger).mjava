	/**
	 * get Num by NumID
	 * @param numID
	 * @return abstractNum with NumId if no Num exists with that NumID 
	 * 			null will be returned
	 */
	public XWPFNum getNum(BigInteger numID){
		for(XWPFNum num: nums){
			if(num.getCTNum().getNumId().equals(numID))
				return num;
		}
		return null;
	}

