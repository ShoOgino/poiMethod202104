        TreeMap<Integer, CTRPrElt> getFormatMap(CTRst entry){
            int length = 0;
            TreeMap<Integer, CTRPrElt> formats = new TreeMap<Integer, CTRPrElt>();
            for (CTRElt r : entry.getRArray()) {
                String txt = r.getT();
                CTRPrElt fmt = r.getRPr();

                length += txt.length();
                formats.put(length, fmt);
            }
            return formats;
        }

