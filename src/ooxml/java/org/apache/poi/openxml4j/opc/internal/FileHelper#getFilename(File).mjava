	/**
	 * Get file name from the specified File object.
	 */
	public static String getFilename(File file) {
		if (file != null) {
			String path = file.getPath();
			int len = path.length();
			int num2 = len;
			while (--num2 >= 0) {
				char ch1 = path.charAt(num2);
				if (ch1 == File.separatorChar)
					return path.substring(num2 + 1, len);
			}
		}
		return "";
	}

