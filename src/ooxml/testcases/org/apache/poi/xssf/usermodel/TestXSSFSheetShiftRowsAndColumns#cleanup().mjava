    /**
     * This method writes the workbook to resultDir/fileName.
     */
    @AfterEach
    public void cleanup() throws IOException {
        final String procName = "TestXSSFSheetRemoveTable.cleanup";
        if (workbook == null) {
            return;
        }

        if(fileName == null) {
            return;
        }

        writeFile(procName);

        workbook.close();
    }

