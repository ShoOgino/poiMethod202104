    @Test
    void testInterleaved2() {

        SortedMap<Long, String> base = new TreeMap<>();
        base.put(1L, "B1");
        base.put(2L, "B2");
        base.put(5L, "B5");
        base.put(6L, "B6");

        SortedMap<Long, String> master = new TreeMap<>();
        master.put(3L, "M3");
        master.put(4L, "M4");
        master.put(7L, "M7");
        master.put(8L, "M8");

        testIteration(new CombinedIterable<>(base, master), "B1", "B2",
                "M3", "M4", "B5", "B6", "M7", "M8");
    }

