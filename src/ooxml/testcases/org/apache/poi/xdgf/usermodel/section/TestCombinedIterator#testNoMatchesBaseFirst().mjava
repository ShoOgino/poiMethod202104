    @Test
    void testNoMatchesBaseFirst() {

        SortedMap<Long, String> base = new TreeMap<>();
        base.put(1L, "B1");
        base.put(2L, "B2");
        base.put(3L, "B3");

        SortedMap<Long, String> master = new TreeMap<>();
        master.put(4L, "M4");
        master.put(5L, "M5");
        master.put(6L, "M6");

        testIteration(new CombinedIterable<>(base, master), "B1", "B2",
                "B3", "M4", "M5", "M6");
    }

