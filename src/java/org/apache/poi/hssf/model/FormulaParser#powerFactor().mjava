   /** Parse and Translate a Math Factor  */
    private ParseNode powerFactor() {
    	ParseNode result = percentFactor();
        while(true) {
            SkipWhite();
            if(look != '^') {
                return result;
            }
            Match('^');
            ParseNode other = percentFactor();
            result = new ParseNode(new PowerPtg(), result, other);
        }
    }

