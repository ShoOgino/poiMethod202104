	public static Ptg createPtg(RecordInputStream in) {
		byte id = in.readByte();
		
		if (id < 0x20) {
			return createBasePtg(id, in);
		}
		
		Ptg  retval = createClassifiedPtg(id, in);

		if (id > 0x60) {
			retval.setClass(CLASS_ARRAY);
		} else if (id > 0x40) {
			retval.setClass(CLASS_VALUE);
		} else {
			retval.setClass(CLASS_REF);
		}

	   return retval;
	}

