    private static double modFor(char type, int len) {
        switch (type) {
        case 'h':
            return 24;
        case 'm':
            return 60;
        case 's':
            return 60;
        case '0':
            return Math.pow(10, len);
        default:
            throw new IllegalArgumentException(
                    "Uknown elapsed time spec: " + type);
        }
    }

