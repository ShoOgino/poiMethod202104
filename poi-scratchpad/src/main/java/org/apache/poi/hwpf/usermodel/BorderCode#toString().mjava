    @Override
    public String toString() {
        return isEmpty() ? "[BRC] EMPTY" :
            "[BRC]\n" +
            "        .dptLineWidth         =  (" + getLineWidth() + " )\n" +
            "        .brcType              =  (" + getBorderType() + " )\n" +
            "        .ico                  =  (" + getColor() + " )\n" +
            "        .dptSpace             =  (" + getSpace() + " )\n" +
            "        .fShadow              =  (" + isShadow() + " )\n" +
            "        .fFrame               =  (" + isFrame() + " )\n";
    }

