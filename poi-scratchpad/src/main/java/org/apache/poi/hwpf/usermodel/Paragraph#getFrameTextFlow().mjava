    private short getFrameTextFlow()
    {
        short retVal = 0;
        if (_props.isFVertical())
        {
            retVal |= 1;
        }
        if (_props.isFBackward())
        {
            retVal |= 2;
        }
        if (_props.isFRotateFont())
        {
            retVal |= 4;
        }
        return retVal;
    }

