	public TextAlign getTextAlign() {
		switch (getValue()) {
			default:
			case LEFT:
				return TextAlign.LEFT;
			case CENTER:
				return TextAlign.CENTER;
			case RIGHT:
				return TextAlign.RIGHT;
			case JUSTIFY:
				return TextAlign.JUSTIFY;
			case DISTRIBUTED:
				return TextAlign.DIST;
			case THAIDISTRIBUTED:
				return TextAlign.THAI_DIST;
		}
	}

