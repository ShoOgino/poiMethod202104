        public WmfArcClosure getArcClosure() {
            switch (getWmfRecordType()) {
                default:
                case arc:
                    return WmfArcClosure.ARC;
                case chord:
                    return WmfArcClosure.CHORD;
                case pie:
                    return WmfArcClosure.PIE;
            }
        }

